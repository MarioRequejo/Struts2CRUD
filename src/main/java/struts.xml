<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>

    <constant name="struts.action.excludePattern" value="Struts2CRUD/ws"/>

	
	<package name="default" extends="struts-default">
		<action name="register" class="com.laboratorio.action.RegisterAction">
			<result name="REGISTER">/register.jsp</result>
		</action>
		<action name="report" class="com.laboratorio.action.ReportAction">
			<result name="REPORT">/report.jsp</result>
		</action>
		<action name="updatedetails" class="com.laboratorio.action.UpdateAction">
			<result name="UPDATE">/update.jsp</result>
		</action>
		<action name="deleterecord" class="com.laboratorio.action.ReportAction">
			<result name="DELETE">/delete.jsp</result>
		</action>
	</package>
</struts>  